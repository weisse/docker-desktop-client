  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node_modules/.bin/webpack-dev-server",
    "dev-electron": "node_modules/.bin/electron . & npm run dev",
    "build": "ENV=production npm run clean-build",
